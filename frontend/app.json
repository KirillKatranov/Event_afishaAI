{
  "expo": {
    "name": "EventAfisha",
    "slug": "EventAfisha",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./shared/assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./shared/assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./shared/assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.shmatozz.strelka"
    },
    "web": {
      "output": "server",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "33c2bb35-f9c8-49db-818f-853c54c5ebc6"
      }
    }
  }
}
